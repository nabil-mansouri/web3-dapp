{
  "name": "contract",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build:mainnet": "rm -rf dist/* && tsc --project tsconfig.mainnet.json && node scripts/replace.js tsconfig.mainnet.json",
    "build:testnet": "rm -rf dist/* && tsc --project tsconfig.testnet.json && node scripts/replace.js tsconfig.testnet.json",
    "build:testnet:noclean": "tsc --project tsconfig.testnet.json && node scripts/replace.js tsconfig.testnet.json",
    "build:local": "rm -rf dist/* && tsc --project tsconfig.local.json && node scripts/replace.js tsconfig.local.json",
    "build:local:noclean": "tsc --project tsconfig.local.json && node scripts/replace.js tsconfig.local.json",
    "test": "npx hardhat test",
    "test:init": "tspath",
    "run:local": "npx hardhat node",
    "deploy:local": "npx hardhat run --network localhost scripts/deploy.ts",
    "deploy:testnet": "npm run build:testnet && npx hardhat run --network ropsten scripts/deploy.ts",
    "deploy:kovan": "npm run build:testnet && npx hardhat run --network kovan scripts/deploy.ts",
    "deploy:mainnet": "npm run build:mainnet && npx hardhat run --network mainnet scripts/deploy.ts"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@walletconnect/web3-provider": "^1.6.6",
    "axios": "^0.24.0",
    "ccxt": "^1.61.16",
    "electron": "^15.3.1",
    "events": "^3.3.0",
    "moralis": "0.0.128",
    "nft.storage": "^3.4.0",
    "nftStorage": "npm:nft.storage@^3.4.0",
    "web3-utils": "^1.6.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/hardhat": "3.0.0",
    "@types/mocha": "^9.0.0",
    "btoa": "^1.2.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eth-gas-reporter": "^0.2.24",
    "ethereum-waffle": "^3.4.0",
    "ethers": "5.5.1",
    "globby": "^11.0.2",
    "hardhat": "^2.6.7",
    "hardhat-erc1820": "^0.1.0",
    "hardhat-gas-reporter": "^1.0.4",
    "jsonic": "^1.0.1",
    "mocha": "^9.1.3",
    "mock-local-storage": "^1.1.17",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.11.0",
    "tspath": "^1.3.7",
    "typechain": "5.2.0",
    "typescript": "^4.4.4",
    "typescript-transform-paths": "^3.3.1",
    "xhr2": "^0.2.1"
  }
}
